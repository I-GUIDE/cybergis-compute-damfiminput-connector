{
    "name": "Dam Flood Inundation Map Connector",
    "container": "damfiminput-connector",
    "execution_stage": "./main.sh",
    "slurm_input_rules": {
   
           "time": {
                "max": 60,
                "min": 5,
                "default_value": 5,
                "step": 5,
                "unit": "Minutes"   
            },
            "memory_per_cpu": {
                "max": 1000,
                "min": 200,
                "default_value": 400,
                "step": 200,
                "unit": "MB"
            }
      
    },
    "require_upload_data": true,
    "param_rules": {
        "workflow_file": {
            "type": "string",
            "require": true
        }
    },
    "supported_hpc": ["anvil_community"],
    "default_hpc": "anvil_community"
}
